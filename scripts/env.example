#!/usr/bin/env bash
# Copy this file to scripts/env.local and update values, then source it:
#   cp scripts/env.example scripts/env.local
#   # edit scripts/env.local
#   source scripts/env.local

# Personal access token or GITHUB_TOKEN with repo read permissions
export GITHUB_TOKEN="ghp_yourTokenHere"

# Repository owner (user or org)
export OWNER="your-org-or-user"

# Repository name
export REPO="your-repo"

# Pull request number to test against
export PR_NUMBER="123"